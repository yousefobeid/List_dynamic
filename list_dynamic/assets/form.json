{
  "formElements": [
    {
      "id": "firstName",
      "type": "textField",
      "label": "First",
      "hint": "Enter your first name",
      "required": true,
      "option": false,
      "maxLength": 30
    },
    {
      "id": "lastName",
      "type": "textField",
      "label": "Last Name",
      "hint": "Enter your last name",
      "required": true,
      "option": false,
      "maxLength": 30
    },
    {
      "id": "email",
      "type": "textField",
      "label": "Email",
      "hint": "Enter your email address",
      "required": true,
      "option": false,
      "maxLength": 50
    },
    {
      "id": "toggleOptions",
      "type": "switch",
      "label": "Options",
      "required": true,
      "option": false
    },
    {
      "id": "phoneNumber",
      "type": "textField",
      "label": "Phone Number",
      "hint": "Enter your phone number",
      "required": false,
      "option": true,
      "maxLength": 15,
      "keyboardType": "phone"
    },
    {
      "id": "city",
      "type": "dropdown",
      "label": "City",
      "required": false,
      "option": true,
      "choose": [
        { "label": "Amman", "value": "amman" },
        { "label": "Zarqa", "value": "zarqa" },
        { "label": "Irbid", "value": "irbid" },
        { "label": "Aqaba", "value": "aqaba" },
        { "label": "Balqa", "value": "balqa" },
        { "label": "Karak", "value": "karak" },
        { "label": "Maan", "value": "maan" },
        { "label": "Mafraq", "value": "mafraq" },
        { "label": "Tafilah", "value": "tafilah" },
        { "label": "Jerash", "value": "jerash" },
        { "label": "Ajloun", "value": "ajloun" },
        { "label": "Madaba", "value": "madaba" }
      ]
    },

    {
      "id": "gender",
      "type": "radio",
      "label": "Gender",
      "required": true,
      "option": false,
      "choose": [
        { "label": "Male", "value": "male" },
        { "label": "Female", "value": "female" }
      ]
    },
    {
      "id": "religion",
      "type": "dropdown",
      "label": "Religion",
      "required": true,
      "option": false,
      "choose": [
        { "label": "Islam", "value": "islam" },
        { "label": "Christianity", "value": "christianity" },
        { "label": "Judaism", "value": "judaism" },
        { "label": "Other", "value": "other" }
      ]
    },
    {
      "id": "birthDate",
      "type": "date_picker",
      "label": "Birth Date",
      "required": true,
      "option": false,
      "hint": "Select your birth date",
      "minYear": 1900,
      "maxYear": 2025
    },

    {
      "id": "button",
      "type": "button",
      "label": "Next",
      "action": "submitForm",
      "required": true,
      "option": false
    }
  ]
}
